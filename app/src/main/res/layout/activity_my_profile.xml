<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/home"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F9F8F5"
    android:paddingBottom="0dp"
    android:padding="16dp">


    <TextView
        android:id="@+id/title"
        android:layout_width="383dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="21dp"
        android:layout_marginTop="64dp"
        android:layout_marginEnd="23dp"
        android:layout_marginBottom="-98dp"
        android:fontFamily="@font/lexend_bold"
        android:gravity="center"
        android:text="meu perfil"
        android:textSize="24sp" />


    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="252dp"
        android:layout_height="251dp"
        android:layout_below="@+id/title"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="118dp"
        android:layout_marginTop="90dp"
        android:layout_marginEnd="120dp"
        app:srcCompat="@drawable/reading" />

    <Button
        android:id="@+id/btnLogout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/btnAgenda"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="60dp"
        android:layout_marginTop="60dp"
        android:layout_marginEnd="60dp"
        android:backgroundTint="#F9F8F5"
        android:fontFamily="@font/lexend_bold"
        android:text="encerrar sessÃ£o"
        android:textColor="#FF5678"
        android:textColorLink="#FF5678"
        android:textSize="20sp"
        app:strokeColor="#FF5678"
        app:strokeWidth="2dp"
        android:padding="14dp"/>

    <Button
        android:id="@+id/btnAgenda"
        android:layout_width="223dp"
        android:layout_height="62dp"
        android:layout_below="@+id/imageView3"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="84dp"
        android:layout_marginTop="59dp"
        android:layout_marginEnd="84dp"
        android:backgroundTint="#F9F8F5"
        android:fontFamily="@font/lexend_bold"
        android:gravity="center"
        android:text="agenda"
        android:textColor="#000"
        android:textColorLink="#F9F8F5"
        android:textSize="20sp"
        app:strokeColor="#000"
        app:strokeWidth="2dp" />


</RelativeLayout>
